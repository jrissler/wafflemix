<h1>Listing portfolios</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Intro</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @portfolios.each do |portfolio| %>
  <tr>
    <td><%= portfolio.title %></td>
    <td><%= portfolio.intro %></td>
    <td><%= portfolio.content %></td>
    <td><%= link_to 'Show', portfolio %></td>
  </tr>
<% end %>
</table>
